{
  "name": "snowpack-start",
  "version": "1.0.0-beta.5",
  "description": "Start a new custom Snowpack application.",
  "keywords": [
    "snowpack",
    "custom",
    "start",
    "init",
    "create"
  ],
  "author": "awu43",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/awu43/snowpack-start.git"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "/dist",
    "/dist-files",
    "/dist-templates"
  ],
  "bin": {
    "snowpack-start": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "node postbuild.js",
    "generate": "node test/generate.js",
    "gen-test": "mocha \"test/src-templates-test.js\" && mocha \"test/package-versions-test.js\"",
    "pretest": "npm run build",
    "test": "mocha \"test/*.test.js\"",
    "coverage": "nyc --reporter=html --reporter=text mocha \"test/*.test.js\""
  },
  "devDependencies": {
    "@types/node": "^15.0.2",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "chai": "^4.3.4",
    "chai-files": "^1.4.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "json5": "^2.2.0",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.6.0",
    "tmp": "^0.2.1"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "commander": "^7.2.0",
    "execa": "^5.0.0",
    "fs-extra": "^9.1.0",
    "prompts": "^2.4.1",
    "typescript": "^4.2.4"
  }
}
